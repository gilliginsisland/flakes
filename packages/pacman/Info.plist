    <key>NSUserNotificationAlertStyle</key>
    <string>banner</string>
    <!-- Needed for hardened runtime signing -->
    <key>LSMinimumSystemVersion</key>
    <string>10.10</string>
    <key>LSUIElement</key>
    <true/>
    <key>NSHighResolutionCapable</key>
    <true/>
